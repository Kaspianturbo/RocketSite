@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model RocketSite.Common.Models.SpaceMission
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <h2>Create SpaceMission</h2>

    <form asp-action="Create" asp-controller="SpaceMission" method="post">
        <div>
            <div class="form-group">
                <label asp-for="Name"></label>
                <input type="text" asp-for="Name" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Status"></label>
                <input asp-for="Status" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Cost"></label>
                <input asp-for="Cost" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Altitude"></label>
                <input asp-for="Altitude" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="StartDate"></label>
                <input asp-for="StartDate" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="EndDate"></label>
                <input asp-for="EndDate" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Rocket.Name"></label>
                <input asp-for="Rocket.Name" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Rocket.Version"></label>
                <input asp-for="Rocket.Version" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-outline-secondary" />
            </div>
        </div>
    </form>

    <div>
        <a asp-controller="SpaceMission" asp-action="Index">Back to list</a>
    </div>
</div>