@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<RocketSite.Common.Models.Rocket>
@{

}
<h2>@ViewData["Title"]</h2>
<p>
    <a asp-controller="Rocket" asp-action="Create">Добавити</a>
</p>
<table class="table">
    <tr>
        <th>Имя</th>
        <th>Возраст</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>@item.Name</td>
    <td>@item.Version</td>
    <td>
        @item.Cost
    </td>
    <td>
        @item.Diameter
    </td>
    <td>
        @item.Height
    </td>
    <td>
        @item.MassToGTO
    </td>
    <td>
        @item.MassToLEO
    </td>
    <td>
        @item.EngineType
    </td>
    <td>
        <a asp-controller="Rocket" asp-action="Edit" asp-route-name="@item.Name" asp-route-version="@item.Version">Змінити</a> |
        <a asp-controller="Rocket" asp-action="Delete" asp-route-name="@item.Name" asp-route-version="@item.Version">Видалити</a>
    </td>
</tr>
    }
</table>